{
  "name": "opensea_push_client",
  "version": "1.0.0",
  "description": "An SDK to receive pushed updates from OpenSea over websocket",
  "main": "lib/index.ts",
  "module": "lib/index.ts",
  "repository": "https://github.com/ProjectOpenSea/pushed-updates.git",
  "author": "Charlie Hulcher",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "nyc ./node_modules/.bin/_mocha --require jsdom-global/register 'tests/**/*.ts'",
    "_test": "mocha tests/**/*.ts --require jsdom-global/register --require ts-node/register --timeout 15000",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prewatch": "npm run lint",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' 'tests/**/*.ts' --write",
    "prettier-check": "prettier --check --config .prettierrc 'src/**/*.ts' 'tests/**/*.ts'",
    "commit-check": "npm run lint && npm run prettier-check && npm run test"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/phoenix": "^1.5.3",
    "@types/sinon": "^10.0.6",
    "@types/ws": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdom": "^18.0.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.1.3",
    "mocha-typescript": "^1.1.17",
    "mock-socket": "^9.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "sinon": "^12.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "phoenix": "^1.6.2",
    "ws": "^8.3.0"
  }
}
